    "supertest": "1.2.0",
    "url-loader": "0.5.x",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "1.14.0"
  },
  "dependencies": {
    "app-module-path": "1.0.x",
    "babel-cli": "^6.3.17",
    "babel-core": "6.3.21",
    "babel-eslint": "6.0.2",
